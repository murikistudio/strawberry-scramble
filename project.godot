[application]

config/name="Strawberry Scramble"
run/main_scene="res://scenes/gui/main/main.tscn"
config/use_custom_user_dir=true
boot_splash/image="res://textures/gui/logo.png"
boot_splash/fullsize=false
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://icon.png"
config/version="0.0.1"
config/build=1
config/author="Muriki Game Studio"
run/show_hud=true

[audio]

default_bus_layout="res://resources/bus_layouts/default_bus_layout.tres"

[autoload]

GuiTransitions="*res://addons/simple-gui-transitions/singleton.gd"
